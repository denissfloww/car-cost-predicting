{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node ./build/server.js",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=test ts-node-dev --respawn ./src/index.ts",
    "drop db": "npx typeorm schema:drop",
    "migrate": "npx typeorm migration:run",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "lint": "eslint --ext .ts .",
    "prettier": "prettier --config .prettierrc --write ./**/*.{js,ts,jsx,tsx}",
    "generate-models": "npx typeorm-model-generator -h localhost -d price-predict -p 5432  -u postgres -x 1 -e postgres -o . -s public"
  },
  "author": "denissfloww",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
